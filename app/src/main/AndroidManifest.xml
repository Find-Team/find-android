<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="find.ui">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.FindUI">
        <activity android:name=".ui.answer.AnswerActivity" />
        <activity android:name=".ui.interview.InterviewActivity" />
        <activity android:name=".ui.profile.ProfileActivity" />
        <activity android:name=".ui.mypage.IntroductionActivity" />
        <activity android:name=".ui.values.ValuesActivity" />
        <activity android:name=".ui.mypage.MyPageActivity" />
        <activity android:name=".ui.main.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>